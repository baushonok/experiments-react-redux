{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "preserve",
    "outDir": "public",
    "types": [
      "@types/react",
      "@types/react-dom",
      "@types/react-router-dom"
    ],
    "baseUrl": "./src"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
