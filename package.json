{
  "name": "tiny-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prettier:write": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "faker": "4.1.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.0.1"
  },
  "devDependencies": {
    "@types/faker": "4.1.5",
    "@types/react": "16.8.19",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.3",
    "husky": "2.4.0",
    "lint-staged": "8.2.0",
    "prettier": "1.18.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.5.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
