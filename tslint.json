{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
    ],
    "rules": {
      "quotemark": [
        true,
        "single",
        "jsx-double"
      ],
      "arrow-parens": false,
      "no-empty": false,
      "no-any": true
    },
    "rulesDirectory": []
  }
  